{
  "name": "goose",
  "version": "0.0.1",
  "description": "a goose app",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --inline",
    "rb": "rm -rf build/",
    "wp": "NODE_ENV='production' webpack",
    "im": "cp -rf images build/",
    "sw": "cp service-worker.js build/service-worker.js",
    "fi": "cp favicon.ico build/favicon.ico",
    "ma": "cp manifest.json build/manifest.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run rb && npm run wp && npm run im && npm run sw && npm run fi && npm run ma",
    "deploy:stage": "npm run build && firebase use stage && firebase deploy",
    "deploy:stage:rules": "firebase use stage && firebase deploy --only database"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsaarikivi/goose.git"
  },
  "keywords": [
    "goose"
  ],
  "author": "Tero & Kaapo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsaarikivi/goose/issues"
  },
  "homepage": "https://github.com/tsaarikivi/goose#readme",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.1",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^3.10.1",
    "postcss-loader": "^1.1.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "firebase": "^3.5.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  }
}